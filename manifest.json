{
  "manifest_version": 2,
  "name": "pokemon-tab",
  "version": "1.0",
  "description": "Displays a random pokemon gif in the center of the new tab page",
  "icons": {
    "48": "icons/icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.mozilla.org/*"
      ],
      "js": [
        "main.js"
      ]
    }
  ]
}
